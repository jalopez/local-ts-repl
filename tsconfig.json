{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "allowJs": false,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "declaration": false,
    "noEmit": true
  },
  "include": ["index.ts"]
}
